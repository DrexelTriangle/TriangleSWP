{% extends 'knp_menu.html.twig' %}

{% block list %}
    {% if item.hasChildren and options.depth is not same as(0) and item.displayChildren %}
        {% import _self as knp_menu %}
        <nav role="navigation" id="nav-frontpage">
            <div class="main-main-menu-container">
                <ul class="frontpage">
                    {{ block('children') }}
                </ul>
            </div>
        </nav>
    {% endif %}
{% endblock %}
